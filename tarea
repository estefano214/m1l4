import discord, random
from discord.ext import commands

bot = commands.Bot(command_prefix='?', intents=discord.Intents.all(), help_command=None)

@bot.command()
async def add(ctx, a: int, b: int):
    await ctx.send(a+b)

@bot.command()
async def help(ctx):
    await ctx.send("Comandos: ?add, ?help")

bot.run("t")
